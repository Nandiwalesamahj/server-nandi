<%= simple_form_for(@advertise) do |f| %>
  <%= f.error_notification %>
  <div class="form-inputs">
    <%= f.input :title, label: "Advertise-Title" %>
    <%= f.input :description, label: "Description-Title" %>
    <p>Photo</p>
    <%= f.file_field :photo %>
    <%= f.input :on_page,collection: [:rajya_sanghatna_page,:zilla_sanghatna_page,:dharmik_places_home,:sanghatna_karya_page,:yuva_sanghatna_page,:bhavishyatil_dhyeya_page,:karmachari_bharti_page,:sandarbhsuchi_page,:samaja_vishayi_page,:shaskiya_yojana_page,:shasan_nirnay_page,:samajik_sanstha_page,:pratikriya_page], label: "Display on Page" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
